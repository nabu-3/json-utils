{
    "type": "library",
    "name": "nabu-3/json-utils",
    "description": "nabu-3 JSON Utils",
    "keywords": [ "nabu-3", "nabu", "class", "library", "minimal", "json", "utils", "tools" ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/nabu-3/json-utils"
        }
    ],
    "version": "3.0.0",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rafael Gutierrez",
            "email": "rgutierrez@nabu-3.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nabu-3/json-utils/issues",
        "email": "contact@nabu-3.com",
        "source": "https://github.com/nabu-3/json-utils"
    },
    "require": {
        "php": ">=7.2",
        "ext-mbstring": ">=7.2",
        "nabu-3/minimal-class": ">=3.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/globals.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
